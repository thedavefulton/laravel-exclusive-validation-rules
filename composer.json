{
    "name": "thedavefulton/laravel-exclusive-validation-rules",
    "description": "Two additional validation rules for use with the Laravel framework",
    "keywords": [
        "laravel",
        "validation",
        "rules"
    ],
    "homepage": "https://github.com/thedavefulton/laravel-exclusive-validation-rules",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "David Fulton",
            "email": "thedave@thedavefulton.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Thedavefulton\\LaravelExclusiveValidationRules\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thedavefulton\\LaravelExclusiveValidationRules\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thedavefulton\\LaravelExclusiveValidationRules\\LaravelExclusiveValidationRulesServiceProvider"
            ]
        }
    }
}
